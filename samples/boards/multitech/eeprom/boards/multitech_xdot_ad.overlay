/ {
    /* These aliases are provided for compatibility with samples */
	aliases {
		eeprom-0 = &eeprom0;
	};
};


&i2c0 {
	status = "okay";
	pinctrl-0 = <&i2c0_scl_p0_6 &i2c0_sda_p0_7>;
	pinctrl-names = "default";
	clock-frequency = <I2C_BITRATE_FAST_PLUS>;

	eeprom0: eeprom@51 {
		compatible = "atmel,at24";
		reg = <0x51>;
		status = "okay";
		size = <DT_SIZE_K(16)>;
		pagesize = <64>;
		address-width = <16>;
		timeout = <5>;
		zephyr,deferred-init;
	};
	zephyr,deferred-init;
};